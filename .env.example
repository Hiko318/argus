# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# API Keys and Tokens
# OPENAI_API_KEY=your_openai_api_key_here
# DJI_API_KEY=your_dji_api_key_here
# MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# Database Configuration
# DATABASE_URL=postgresql://user:password@localhost:5432/foresight
# POSTGRES_PASSWORD=your_secure_postgres_password
# REDIS_URL=redis://localhost:6379

# Service Configuration
# SAR_SERVICE_PORT=8004
# DETECTION_CONFIDENCE_THRESHOLD=0.5
# GEOLOCATION_PRECISION=high

# Logging
# LOG_LEVEL=INFO
# LOG_FILE_PATH=./out/logs/app.log

# Security
# JWT_SECRET=your_jwt_secret_here
# ENCRYPTION_KEY=your_encryption_key_here

# External Services
# WEATHER_API_KEY=your_weather_api_key_here
# TERRAIN_SERVICE_URL=https://api.terrain-service.com

# Development Settings
# DEBUG=false
# ENABLE_TELEMETRY=true
# MOCK_DRONE_DATA=false